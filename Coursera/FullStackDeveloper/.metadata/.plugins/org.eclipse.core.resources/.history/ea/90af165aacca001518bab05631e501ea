<html>
	<head>
		<title>Guessing Color game</title>
		<script type="text/javascript">
			
			var target;
			var guess_input_color;
			var guess_input;
			var finished = false;
			var guesses = 0;
			
			function do_game(){
				var ramdom = Math.random() * 100;
				var ramdom_int = Math.floor(ramdom);
				target = ramdom_int + 1;
				
				while(!finished){
					guess_input_color = "I am thinking of a color " +
					"guess the color that I'm thinking";
					
					guess_input = parseInt(guess_input_color);
					guesses +=1;
					finished = check_guess();
				}
			}
			
			function check_guess(){
				if(isNan(guess_input)){
					alert("You have enter a non number");
					return false;
				}
				if(isNan(guess_input)){
					alert("You have enter a non number");
					return false;
				}
				if(isNan(guess_input)){
					alert("You have enter a non number");
					return false;
				}
				if(isNan(guess_input)){
					alert("You have enter a non number");
					return false;
				}
				
				alert("Is that one!");
				
				return true;
				
			}
		
		</script>
	</head>
	<body onload="do_game()" id="bodyhtml">
	
	
	</body>
</html>